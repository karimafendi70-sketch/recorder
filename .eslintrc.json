{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "script"
  },
  "globals": {
    "SURF_SPOTS": "readonly"
  },
  "rules": {
    "no-unused-vars": [
      "warn",
      {
        "args": "none",
        "caughtErrors": "none"
      }
    ]
  }
}
